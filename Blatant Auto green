local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()

local window = DrRayLibrary:Load("blatant Auto green", "Default")

local tab = DrRayLibrary.newTab("blatant Auto green", "ImageIdHere")

local toggle = tab.newToggle("Blatant Auto Green", "Turns on the script", function(state)
if state then
getgenv().greenNumber = 90.34 -- keep this number around 90
getgenv().unlockfps = true -- recommend
getgenv().keytoclick = "E" -- don't change this
getgenv().customautogreen = Enum.KeyCode.R -- press R if you don't want to hold shoot button

print("Blatant Auto Green: Enabled")
else
getgenv().greenNumber = nil
getgenv().unlockfps = nil
getgenv().keytoclick = nil
getgenv().customautogreen = nil

print("Blatant Auto Green: Disabled")
end
end)
